{
	"Name": "OpenDeck Starter Pack",
	"Description": "OpenDeck starter pack plugin",
	"Author": "ninjadev64",
	"Version": "1.2.0",
	"CodePathWin": "opendeck-starterpack.exe",
	"CodePathMac": "opendeck-starterpack-mac",
	"CodePathLin": "opendeck-starterpack",
	"Icon": "icons/plugin",
	"Category": "OpenDeck",
	"OS": [
		{
			"Platform": "mac",
			"MinimumVersion": "10.11"
		},
		{
			"Platform": "windows",
			"MinimumVersion": "10"
		},
		{
			"Platform": "linux"
		}
	],
	"Actions": [
		{
			"Icon": "icons/runCommand",
			"Name": "Run command",
			"States": [ { "Image": "actionDefaultImage" } ],
			"Controllers": [ "Keypad" ],
			"SupportedInMultiActions": true,
			"Tooltip": "Run a command",
			"UUID": "com.amansprojects.starterpack.runcommand",
			"PropertyInspectorPath": "propertyInspector/runCommand.html"
		},
		{
			"Icon": "icons/runCommand",
			"Name": "Run command toggle",
			"States": [ { "Image": "actionDefaultImage", "Title": "State 1", "ShowTitle": true }, { "Image": "actionDefaultImage", "Title": "State 2", "ShowTitle": true } ],
			"Controllers": [ "Keypad" ],
			"SupportedInMultiActions": true,
			"Tooltip": "Run a command with 2 toggleable states",
			"UUID": "com.amansprojects.starterpack.runcommandtoggle",
			"PropertyInspectorPath": "propertyInspector/runCommandToggle.html"
		},
		{
			"Icon": "icons/inputSimulation",
			"Name": "Input simulation",
			"States": [ { "Image": "actionDefaultImage" } ],
			"Controllers": [ "Keypad" ],
			"SupportedInMultiActions": true,
			"Tooltip": "Simulate mouse or keyboard input",
			"UUID": "com.amansprojects.starterpack.inputsimulation",
			"PropertyInspectorPath": "propertyInspector/inputSimulation.html"
		},
		{
			"Icon": "icons/switchProfile",
			"Name": "Switch profile",
			"States": [ { "Image": "actionDefaultImage" } ],
			"Controllers": [ "Keypad" ],
			"SupportedInMultiActions": true,
			"Tooltip": "Switch the selected profile",
			"UUID": "com.amansprojects.starterpack.switchprofile",
			"PropertyInspectorPath": "propertyInspector/switchProfile.html"
		}
	]
}
